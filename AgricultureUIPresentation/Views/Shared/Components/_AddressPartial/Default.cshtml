@using EntityLayer.Concrete
@model List<Address>


<h3 class="tittle">Bize Ulaşın</h3>
<p class="heading-btm-w3ls">Bize aşağıdaki bilgiler aracılığı ile vaya formu doldurarak ulaşabilirsiniz.</p>

<div class="col-md-3 contact-left">
    <h4>Adresimiz</h4>

    @foreach (var item in Model)
    {

        <p>@item.Desc1,</p>
        <p>@item.Desc2,</p>
        <p>@item.Desc3</p>
        <h4>Telefon</h4>
        <p>@item.Desc4</p>


    }
</div>
